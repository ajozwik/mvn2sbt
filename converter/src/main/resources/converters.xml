<?xml version="1.0" encoding="UTF-8"?>
<converters xmlns="https://github.com/ajozwik/mvn2sbt">
    <converter>
        <artifactId>cxf-codegen-plugin</artifactId>
        <sbtSetting>cxf.settings</sbtSetting>
        <pluginsSbtPluginConfiguration>
            <dependency>
                <groupId>com.ebiznext.sbt.plugins</groupId>
                <artifactId>sbt-cxf-wsdl2java</artifactId>
                <version>0.1.4</version>
            </dependency>
        </pluginsSbtPluginConfiguration>
        <extraRepository>"Sonatype Repository" at "https://oss.sonatype.org/content/groups/public"</extraRepository>
        <dependencies/>
        <converter>pl.jozwik.mvn2sbt.pom2sbt.CxfPluginConverter</converter>
    </converter>

    <converter>
        <artifactId>gmaven-plugin</artifactId>
        <sbtSetting>org.softnetwork.sbt.plugins.GroovyPlugin.groovy.settings</sbtSetting>
        <pluginsSbtPluginConfiguration>
            <dependency>
                <groupId>org.softnetwork.sbt.plugins</groupId>
                <artifactId>sbt-groovy</artifactId>
                <version>0.1</version>
            </dependency>
        </pluginsSbtPluginConfiguration>
        <extraRepository>"Biblio" at "http://mirrors.ibiblio.org/maven2"</extraRepository>
        <converter/>
    </converter>
    <converter>
        <artifactId>jaxb2-maven-plugin</artifactId>
        <sbtSetting>com.github.retronym.sbtxjc.SbtXjcPlugin.xjcSettings</sbtSetting>
        <pluginsSbtPluginConfiguration>
            <dependency>
                <groupId>org.scala-sbt.plugins</groupId>
                <artifactId>sbt-xjc</artifactId>
                <version>0.5</version>
            </dependency>
        </pluginsSbtPluginConfiguration>
        <extraRepository>Resolver.url("scalasbt",
            url("http://scalasbt.artifactoryonline.com/scalasbt/repo"))(Resolver.ivyStylePatterns)
        </extraRepository>
        <converter>pl.jozwik.mvn2sbt.pom2sbt.JaxbPluginConverter</converter>
    </converter>
    <converter>
        <artifactId>maven-thrift-plugin</artifactId>
        <sbtSetting>com.github.bigtoast.sbtthrift.ThriftPlugin.thriftSettings</sbtSetting>
        <pluginsSbtPluginConfiguration>
            <dependency>
                <groupId>com.github.bigtoast</groupId>
                <artifactId>sbt-thrift</artifactId>
                <version>0.7</version>
            </dependency>
        </pluginsSbtPluginConfiguration>
        <extraRepository>"bigtoast-github" at "http://bigtoast.github.com/repo/"</extraRepository>
        <dependencies>
            <dependency>
                <groupId>commons-lang</groupId>
                <artifactId>commons-lang</artifactId>
                <version>2.6</version>
            </dependency>
        </dependencies>
        <converter/>
    </converter>
    <converter>
        <artifactId>maven-war-plugin</artifactId>
        <sbtSetting>webSettings</sbtSetting>
        <pluginsSbtPluginConfiguration>
            <dependency>
                <groupId>com.earldouglas</groupId>
                <artifactId>xsbt-web-plugin</artifactId>
                <version>0.9.0</version>
            </dependency>
        </pluginsSbtPluginConfiguration>
        <dependencies>
            <dependency>
                <groupId>org.eclipse.jetty</groupId>
                <artifactId>jetty-webapp</artifactId>
                <version>9.1.0.v20131115</version>
                <scope>container</scope>
            </dependency>
            <dependency>
                <groupId>org.eclipse.jetty</groupId>
                <artifactId>jetty-plus</artifactId>
                <version>9.1.0.v20131115</version>
                <scope>container</scope>
            </dependency>
        </dependencies>
        <converter/>
    </converter>
</converters>